  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks

  # Pods for termination_project

  target 'SmartparkTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SmartparkUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
